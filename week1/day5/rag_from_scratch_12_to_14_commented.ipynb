   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "729074f9-8bde-4c76-a7da-4cc0e50ed52d",
   "metadata": {},
   "outputs": [],
   "source": [
    "# 使用多向量检索器获取完整文档\n",
    "# 检索器会根据摘要找到对应的原始文档\n",
    "retrieved_docs = retriever.get_relevant_documents(query, n_results=1)\n",
    "\n",
    "print(f\"通过多向量检索器获取的完整文档:\")\n",
    "print(f\"内容预览: {retrieved_docs[0].page_content[:500]}...\")\n",
    "print(f\"文档长度: {len(retrieved_docs[0].page_content)} 字符\")\n",
    "print(f\"检索到的文档数量: {len(retrieved_docs)}\")"
   ]
  },
